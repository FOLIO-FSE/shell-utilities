tenant=$(cat tenant)
okapi_url=$(cat okapi.url)
okapi_token=$(cat okapi.token)

curl -s -w '\n' -X GET -H "Accept: application/json" -H "X-Okapi-Tenant: ${tenant}" -H "x-okapi-token: ${okapi_token}" "${okapi_url}/contributor-name-types" |jq -c . |tee "contributor-nametypes.json"

echo "Contributor name types have been written to contributor-nametypes.json"

tenant=$(cat tenant)
okapi_url=$(cat okapi.url)
okapi_token=$(cat okapi.token)

user_uuid=$(sed 's/[^a-z0-9\-]//g' <<< $1)

apicall=$(curl -s -w '\n' -X GET -H "Accept: application/json" -H "X-Okapi-Tenant: ${tenant}" -H "x-okapi-token: ${okapi_token}" "${okapi_url}/circulation/loans?limit=100000&query=userId%3D%3D${user_uuid}")

echo $apicall |jq .
